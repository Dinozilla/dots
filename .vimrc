set nu rnu
set showmatch

set smartcase
set ignorecase
set incsearch

set autoindent
set cindent
set shiftwidth=4
set smartindent
set smarttab
set softtabstop=4

set ruler
set backspace=indent,eol,start

set mouse=a
set encoding=utf8
set noexpandtab

syntax on

set clipboard=unnamed
set clipboard=unnamedplus
